;
; BIND reverse data file for greennet
;
$TTL	604800
@	IN	SOA	ns1.greennet.com. router.greennet.com. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name servers - NS records
	IN	NS	ns1.greennet.com.
; PTR records
1	IN	PTR	ns1.greennet.com. ; 192.168.0.1
15	IN	PTR	a.greennet.com.	 ; 192.168.0.15
10	IN	PTR	b.greennet.com.	 ; 192.168.0.10
