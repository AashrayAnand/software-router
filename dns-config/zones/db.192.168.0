;
; BIND reverse data file for bluenet
;
$TTL	604800
@	IN	SOA	ns1.bluenet.com. router.bluenet.com. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name servers - NS records
	IN	NS	ns1.bluenet.com.
; PTR records
1	IN	PTR	ns1.bluenet.com. ; 192.168.0.1
15	IN	PTR	a.bluenet.com.	 ; 192.168.0.5
10	IN	PTR	b.bluenet.com.	 ; 192.168.0.10
